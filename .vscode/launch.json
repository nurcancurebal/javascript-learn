{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": true,
            "args": [],
            "cwd": "${fileDirname}",
            "runtimeExecutable": null,
            "console": "integratedTerminal",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}